{
  "name": "ishine-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:ssg": "cross-env SSG=true tsc && vite-ssg build",
    "sitemap": "node scripts/generate-sitemap.js",
    "build:sitemap": "npm run sitemap && npm run build",
    "deploy": "npm run build:sitemap",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "check-redirects": "node scripts/check-redirects.js",
    "test-url": "node scripts/check-redirects.js"
  },
  "dependencies": {
    "framer-motion": "^10.12.18",
    "lucide-react": "^0.259.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.9",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-helmet-async": "^1.0.1",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "postcss": "^8.4.25",
    "tailwindcss": "^3.3.2",
    "terser": "^5.43.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.0",
    "vite-ssg": "^0.23.0"
  }
